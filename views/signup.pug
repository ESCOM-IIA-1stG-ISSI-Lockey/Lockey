extends layout_sign

block sign_block
	main
		p signup
		section
			form
				p A
				label Nombre completo:
				input(type="text")
				label Numero de Celular:
				input(type="tel" id="celular" name="celular" pattern="[0-9]{2}-[0-9]{4}-[0-9]{4}") 
				label Correo electronico: 
				input(type="email") 
				label Contraseña:
				input(type="password")
				label Confirmar Contraseña:
				input(type="password")