{
	"name": "lockey",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "nodemon ./bin/www",
		"dev": "nodemon ./bin/www",
		"prestart": "sass --watch --poll ./sass:./public/stylesheets &"
	},
	"dependencies": {
		"body-parser": "^1.20.1",
		"bootstrap": "^5.2.3",
		"cookie-parser": "~1.4.3",
		"debug": "~2.6.0",
		"express": "^4.18.2",
		"morgan": "^1.10.0",
		"pug": "^3.0.2",
		"serve-favicon": "^2.5.0"
	},
	"devDependencies": {
		"nodemon": "^2.0.20",
		"sass": "^1.56.1"
	},
	"nodemonConfig": {
		"legacyWatch": true,
		"delay": 1500,
		"ignore": [ "node_modules", "public" ],
		"ext": "js,scss,env",
		"watch": [
			"."
		],
		"execMap": {
			"scss": "sass ./sass:./public/stylesheets"
		}
	}
}
